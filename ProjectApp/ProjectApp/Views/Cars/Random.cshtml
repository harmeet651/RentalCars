@model ProjectApp.ViewModels.RandomCarViewModel
@{
    ViewBag.Title = "Random";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@if (Model.ListCars.Count == 0)
{
    <p>No one has rented this car.</p>
}
else
{
    <ul>
        @foreach (var car in Model.ListCars)
        {
            <li>
                @car.Name
            </li>
        }
    </ul>
}



