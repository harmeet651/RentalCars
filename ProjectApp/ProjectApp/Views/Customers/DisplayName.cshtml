@model ProjectApp.Models.Customer
@{
    ViewBag.Title = Model;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>@Model.Name<h2>

    @if (!Model.MembershipType.Any())
    {
        <h6> ...........Doesn't have a membership yet.</h6>
    }
    else
    {
        <ul>
            <li><h5>@Model.MembershipType</h5></li>
        </ul>
    }

    @if (!Model.Owns.Any())
    {
        <td>..No Car..</td>
    }
    else
    {
        <td>@Model.Owns</td>
    }
